{
  "manifest_version": 2,

  "name": "Your Extension Here",
  "description": "Powered by Mutant",
  "version": "1.0",

  "permissions": [],
  "browser_action": {
    "default_title": "Sidekick",
    "default_icon": "sidekick/chemistry.png",
    "default_popup": "sidekick/popup.html"
    },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": [],
      "js": [
        "plugins/jquery.js",
        "plugins/jquery-ui.js",
        "plugins/underscore.js",
        "plugins/less.js",
        "plugins/purl.js",
        "rules.json",
        "plugins/mutant.js"
      ]
    }
  ],
  "web_accessible_resources": [ // Don't forget to include these in rules.json!
  	"example/style.css",
    "example/script.js",
    "nyt/style.css",
    "nyt/script.js",
    "sidekick/popup.css",
    "sidekick/popup.js"
  ]

}